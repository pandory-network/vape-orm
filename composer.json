{
  "name": "pandory-network/vape-orm",
  "description": "Simple ORM.",
  "keywords": [
    "simple",
    "orm",
    "database",
    "objects",
    "entity"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Benjamin von Minden",
      "email": "github@pandory.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "doctrine/annotations": "1.2.*",
    "doctrine/collections": "1.3.*"
  },
  "autoload": {
    "psr-4": {
      "VapeORM\\": "src"
    }
  },
  "archive": {
    "exclude": [
      "!vendor",
      "tests",
      "*phpunit.xml",
      ".travis.yml",
      ".coveralls.yml",
      "composer.phar"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "stable"
}